{
    "name": "haught/laravel-saml2",
    "description": "A Laravel package for Saml2 integration as a SP (service provider) for multiple IdPs, based on OneLogin toolkit which is much more lightweight than simplesamlphp.",
    "keywords": ["laravel","saml", "saml2", "onelogin"],
    "homepage": "https://github.com/haught/laravel-saml2",
    "license": "MIT",
    "version": "2.1.0",
    "authors": [
        {
            "name": "haught",
            "email": "matt@haught.org"
        },
        {
            "name": "aacotroneo",
            "email": "aacotroneo@gmail.com"
        },
        {
            "name": "Niraj Patkar",
            "email": "niraj@alphonso.in"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-openssl": "*",
        "laravel/framework": ">=8.0.0",
        "onelogin/php-saml": ">=4.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.5",
        "phpunit/phpunit": "~9"
    },
    "autoload": {
        "psr-0": {
            "Haught\\Saml2\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Haught\\Saml2\\Saml2ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
